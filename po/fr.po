#: src/bin/calaos_server/Audio/AVReceiver.cpp:222
msgid "AVReceiver object to control network amplifier"
msgstr "Objet AVReceiver pour contrôler l'amplificateur réseau"

#: src/bin/calaos_server/Audio/AVReceiver.cpp:223
msgid "IP address of the device"
msgstr "Adresse IP de l'appareil"

#: src/bin/calaos_server/Audio/AVReceiver.cpp:224
msgid "Port to use for connection"
msgstr "Port pour la connexion"

#: src/bin/calaos_server/Audio/AVReceiver.cpp:225
msgid "Zone of the amplifier (if supported)"
msgstr "Zone de l'amplificateur (si pris en charge)"

#: src/bin/calaos_server/Audio/AVReceiver.cpp:226
msgid "AVReceiver model. Supported: pioneer, denon, onkyo, marantz, yamaha"
msgstr ""
"Modèle AVReceiver. Pris en charge : pionnier, denon, onkyo, marantz, yamaha"

#: src/bin/calaos_server/Audio/AVReceiver.cpp:228
msgid "Switch receiver on"
msgstr "Allumer le récepteur"

#: src/bin/calaos_server/Audio/AVReceiver.cpp:229
msgid "Switch receiver off"
msgstr "Eteindre le récepteur"

#: src/bin/calaos_server/Audio/AVReceiver.cpp:230
#: src/bin/calaos_server/Audio/AudioPlayer.cpp:43
msgid "Set current volume"
msgstr "Régler le volume"

#: src/bin/calaos_server/Audio/AVReceiver.cpp:231
msgid "Change current input source"
msgstr "Changer l'entrée actuelle"

#: src/bin/calaos_server/Audio/AVReceiver.cpp:232
msgid "Send a custom command to receiver (if you know the protocol)"
msgstr ""
"Envoyer une commande personnalisée au récepteur (si vous connaissez le "
"protocole)"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:31
msgid "Event when play is started"
msgstr "Événement lors de la lecture"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:32
msgid "Event when pausing player"
msgstr "Événement lors de la pause"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:33
msgid "Event when stopping player"
msgstr "Événement lors de l'arrêt"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:34
msgid "Event when a new song is being played"
msgstr "Événement lors de la lecture d'une nouvelle chanson"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:35
msgid "Event when a change in the current playlist happens"
msgstr "Événement lors d'une modification de la liste de lecture"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:36
msgid "Event when a change of volume happens"
msgstr "Événement lors d'un changement de volume"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:38
msgid "Start playing"
msgstr "Démarrer la lecture"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:39
msgid "Pause player"
msgstr "Suspendre la lecture"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:40
msgid "Stop player"
msgstr "Arrêter la lecture"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:41
msgid "Play next song in playlist"
msgstr "Lire la prochaine chanson de la liste de lecture"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:42
msgid "Play previous song in playlist"
msgstr "Lire la précédente chanson de la liste de lecture"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:44
msgid "Increase volume by a value"
msgstr "Augmenter le volume"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:45
msgid "Decrease volume by a value"
msgstr "Diminuer le volume"

#: src/bin/calaos_server/Audio/AudioPlayer.cpp:47
msgid "Audio players are not displayed in rooms"
msgstr "Les lecteurs audio ne sont pas affichés dans les pièces"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:46
msgid "Squeezebox audio player allows control of a Squeezebox from Calaos"
msgstr ""
"Le lecteur audio Squeezebox permet de contrôler une Squeezebox depuis Calaos"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:48
msgid "Logitech media server IP address"
msgstr "Adresse IP du serveur multimédia Logitech"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:49
msgid "Unique ID of squeezebox in LMS"
msgstr "ID unique de la squeezebox sur le serveur"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:50
msgid "CLI port of LMS, default to 9090"
msgstr "Port du serveur, 9090 par défaut"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:51
msgid "Web interface port of LMS, default to 9000."
msgstr "Port de l'interface Web, 9000 par défaut"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:53
msgid "Switch player on"
msgstr "Allumer le lecteur"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:54
msgid "Switch player off"
msgstr "Eteindre le lecteur"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:55
msgid "Start sleep mode with X seconds"
msgstr "Mettre en veille dans X secondes"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:56
msgid "Sync this player with an other"
msgstr "Synchronisez ce lecteur avec un autre"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:57
msgid "Stop sync of this player with an other"
msgstr "Arrêtez la synchronisation de ce lecteur"

#: src/bin/calaos_server/Audio/Squeezebox.cpp:58
msgid ""
"Clear playlist and play argument. <argument> can be any of album_id:XX "
"artist_id:XX playlist_id:XX, ..."
msgstr ""
"Effacer la liste de lecture et lire argument. <argument> peut être n'importe "
"quel album_id: XX artist_id: XX playlist_id: XX, ..."

#: src/bin/calaos_server/Audio/Squeezebox.cpp:59
msgid ""
"Add tracks to playlist. <argument> can be any of album_id:XX artist_id:XX "
"playlist_id:XX, ..."
msgstr ""
"Ajouter des pistes à la liste de lecture. <argument> peut être n'importe "
"quel album_id: XX artist_id: XX playlist_id: XX, ..."

